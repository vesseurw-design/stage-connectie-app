{
  "buildCommand": "npm run build",
  "outputDirectory": "build",
  "env": {
     "REACT_APP_SUPABASE_URL": "REACT_APP_SUPABASE_URL",
     "REACT_APP_SUPABASE_ANON_KEY": "REACT_APP_SUPABASE_ANON_KEY"
  },
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=3600, s-maxage=3600"
        }
      ]
    }
  ]
}
